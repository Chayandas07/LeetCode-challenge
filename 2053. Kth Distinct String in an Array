class Solution {
public:
    string kthDistinct(vector<string>& arr, int k) {
       unordered_map<string, int> strFreqMp;
       for(auto &str : arr)strFreqMp[str]++; 
       for(auto &str : arr){
        if(strFreqMp[str] == 1)k--;
        if(k == 0)return str;
       }
       return "";
    }
};
